rySelector('.form__input--elevation')],\n [\"formRowHidden\",\"form__row--hidden\"],\n [\"crossClass\", \".cross\"],\n [\"workoutClass\", \".workout\"]\n])\n\n\nexport const workoutHtmlString = (workout) => {\n  let html = `\n    <li class=\"workout workout--${workout.type}\" data-id=\"${workout.id}\">\n    <div class=\"cross\">\n    <span class=\"cross__bar cross__bar--1\"></span>\n    <span class=\"cross__bar cross__bar--2\"></span>\n    </div>\n    <h2 class=\"workout__title\">${workout.description}</h2>\n    <div class=\"workout__details\">\n    <span class=\"workout__icon\">${workout.type === 'running' ? 'üèÉ‚Äç‚ôÇÔ∏è' : 'üö¥‚Äç‚ôÄÔ∏è'}</span>\n    <span class=\"workout__value\">${workout.distance}</span>\n    <span class=\"workout__unit\">km</span>\n    </div>\n    <div class=\"workout__details\">\n    <span class=\"workout__icon\">‚è±</span>\n    <span class=\"workout__value\">${workout.duration}</span>\n    <span class=\"workout__unit\">min</span>\n    </div>\n    <div class=\"workout__details\">\n    <span class=\"workout__icon\">‚ö°Ô∏è</span>\n    <span class=\"workout__value\">${workout.speed}</span>\n    <span class=\"workout__unit\">km/h</span>\n    </div> `\n\n  if (workout.type === \"cycling\") {\n    html+=\n    `<div class=\"workout__details\">\n    <span class=\"workout__icon\">‚õ∞</span>\n    <span class=\"workout__value\">${workout.elevationGain}</span>\n    <span class=\"workout__unit\">m</span>\n    </div>\n    </li> `\n  } else {\n    html +=\n    `\n    <div class=\"workout__details\">\n    <span class=\"workout__icon\">ü¶∂üèº</span>\n    <span class=\"workout__value\">${workout.cadence}</span>\n    <span class=\"workout__unit\">spm</span>\n    </div>\n    `\n  }\n  return html;\n}\n\n\nexport const getLocation = () => {\n  return new Promise((resolve, reject) => {\n      navigator.geolocation.getCurrentPosition((success) => {\n          const {latitude, longitude} = success.coords;\n          resolve({latitude, longitude})\n      }, error => {\n          reject(error)\n      })\n  })\n}\n"},"lineCount":null}},"hash":"e31ac7e587fc2f90909e39bb59c9857c","cacheData":{"env":{}}}